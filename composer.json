{
    "name": "gazatem/intellect",
    "description": "Laravel Based open source code business software",
    "type": "project",
    "keywords": ["inventory", "inventory-management", "stock-prices", "order-management", "business-intelligence", "laravel", "php", "manage-purchases"],
    "homepage": "https://gazatem.com",
    "license": "MIT",
    "authors": [
        {
            "name": "gazatem",
            "email": "info@gazatem.com",
            "homepage": "https://www.gazatem.com"
        }
    ],
 
    "require": {
        "php": ">=7.2",
        "albertcht/invisible-recaptcha": "^1.9",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^7.0",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "lcobucci/jwt": "^3.3",
        "maatwebsite/excel": "^3.1",
        "mbezhanov/faker-provider-collection": "^1.2",
        "spatie/laravel-html": "^2.24",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "1.2.*",
        "phpunit/phpunit": "^8.1",
        "spatie/phpunit-watcher": "^1.12"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
